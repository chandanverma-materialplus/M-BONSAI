{
  "name": "m-plus-bonsai",
  "version": "1.0.0",
  "description": "AI Platform Architecture - Multi-agent collaboration with vector databases and integrations",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
